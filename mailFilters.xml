		<updated>2015-03-03T13:57:46Z</updated>
		<content></content>
		<apps:property name='hasTheWord' value='subject:(wfh OR WFH OR Wfh OR &quot;2hr delay&quot; OR PTO OR VTO OR &quot;In by&quot; OR &quot;In befor&quot; OR &quot;finishing from&quot; OR &quot;working from&quot;)'/>
		<apps:property name='shouldMarkAsRead' value='true'/>
